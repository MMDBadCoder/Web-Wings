{
    "manifest_version": 3,
    "name": "Session Injector",
    "version": "1.0",
    "description": "Give a session ID and inject corresponding cookie.",
    "permissions": [
        "cookies",
        "storage",
        "activeTab",
        "scripting"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icons/injection16.png",
            "48": "icons/injection48.png",
            "128": "icons/injection128.png"
        }
    },
    "background": {
        "service_worker": "background.js"
    }
}